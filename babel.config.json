{
	"presets": [
		["@babel/preset-env", {
			"debug": true
		}]
	],
	"targets": {
		"node": "current"
	},
	"plugins": [
		[
			"@babel/plugin-transform-runtime",
			{
				"regenerator": true
			}
		],
		"@babel/plugin-proposal-async-generator-functions",
		"@babel/plugin-proposal-object-rest-spread"
	]
}
